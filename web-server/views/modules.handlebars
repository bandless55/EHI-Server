<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar">
      <ul class="nav nav-sidebar">
        {{#each sidebar.modules}}
          <li class={{ifActive @index}}><a href="{{@index}}">{{metaData.name}}</a></li>
        {{/each}}
      </ul>
    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <h1 class="page-header">{{current.metaData.name}} Module</h1>
        {{#if current.metaData.graphs}}
          <h2 class="sub-header">Graphs</h2>
            {{> graph current.metaData.graphs}}
        {{/if}}
    </div>
  </div>
</div>